{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target":"ES6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "outDir": "./dist",
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
            "*": [
                "../node_modules/*"
            ]
        }
    },
    "include": [
        "./src/**/*.ts"
    ],
    "exclude": [
        "src/front"
    ],
}